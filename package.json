{
  "name": "i18n-webpack-plugin",
  "version": "1.0.0",
  "author": "Tobias Koppers @sokra",
  "description": "Embed localization into your bundle",
  "main": "dist/cjs.js",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "start": "npm run build -- -w",
    "appveyor:test": "npm run test",
    "build": "cross-env NODE_ENV=production babel src -d dist --ignore 'src/**/*.test.js'",
    "clean": "del-cli dist",
    "clean:dist": "del-cli dist",
    "lint": "eslint --cache src test",
    "lint-staged": "lint-staged",
    "prebuild": "npm run clean",
    "prepublish": "npm run build",
    "release": "standard-version",
    "serve:dev": "nodemon $2 --exec babel-node",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --collectCoverageFrom='src/**/*.js' --coverage",
    "travis:coverage": "npm run test:coverage -- --runInBand",
    "travis:lint": "npm run lint && npm run security",
    "travis:test": "npm run test -- --runInBand",
    "webpack-defaults": "webpack-defaults"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "babel-jest": "^24.9.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^6.0.3",
    "del-cli": "^3.0.0",
    "eslint": "^6.6.0",
    "eslint-config-webpack": "^1.2.5",
    "eslint-plugin-import": "^2.18.2",
    "jest": "^25.0.0",
    "lint-staged": "^9.4.2",
    "pre-commit": "^1.2.2",
    "standard-version": "^7.0.0",
    "webpack": "^4.41.2",
    "webpack-defaults": "^4.0.0"
  },
  "peerDependencies": {
    "webpack": "^4.0.0"
  },
  "engines": {
    "node": ">= 6.9.0 || >= 8.9.0"
  },
  "homepage": "https://webpack.js.org",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webpack-contrib/i18n-webpack-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/webpack-contrib/i18n-webpack-plugin/issues"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {}
}
